//- ****************** KEEP AUDIO SOURCE *********************
#soundBoard
	.pill(v-for='item in items')
		span.command
			p {{item.message}}
		span.copy.pillButton(data-clipboard-text='{{item.message}}')
			p.material-icons content_copy
			.tooltip.tooltipBlue
				p Copy Command
		span.preview.pillButton#pill(v-on:click='getEL')
			p.material-icons play_arrow
			audio(src="{{item.audioSrc}}")
			.tooltip.tooltipRed
				p Play Sound
